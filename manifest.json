{
    "manifest_version": 3,
    "name": "Wordle",
    "version": "0.2.0",
    "description": "Play Wordle In An Extension",
    "author": "Dylan Soll",
    "action":{
        "default_popup": "popup.html",
        "default_title": "Wordle Ext"
    },
    "permissions": ["storage", "scripting"],
    "content_scripts": [
      {"matches": ["*://*/*"],
      "css": ["popup_style.css"],
      "js": ["main.js", "5l_words.js","wordle_functions.js", "onclick_functions.js", "misc_functions.js"]}
    ],
    "icons": {
   }
    
}