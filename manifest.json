{
    "manifest_version": 3,
    "name": "Robot Controller",
    "version": "0.0.1",
    "description": "Send messages to your friends",
    "author": "Dylan Soll",
    "action":{
        "default_popup": "popup.html",
        "default_title": "Robot Unlocker",
        
    "permissions": ["storage", "scripting", "activeTab"]
    },
    "content_scripts": [
      {"matches": ["*://*/*"],
      "css": ["popup_style.css"],
      "js": ["popup_function.js"]}
    ],
    "icons": {
      "16": "icons/padlock16.png"
   }
    
}